gcloud beta compute health-checks create tcp hola-mundo-health-check --project=misw4204-sw-en-la-nube --port=5000 --proxy-header=NONE --no-enable-logging --check-interval=10 --timeout=5 --unhealthy-threshold=3 --healthy-threshold=2 && gcloud beta compute instance-groups managed create hola-mundo-group --project=misw4204-sw-en-la-nube --base-instance-name=hola-mundo-group --template=projects/misw4204-sw-en-la-nube/regions/us-central1/instanceTemplates/instance-template-webholamundo --size=1 --zones=us-central1-c,us-central1-f,us-central1-b --target-distribution-shape=EVEN --instance-redistribution-type=PROACTIVE --default-action-on-vm-failure=repair --health-check=projects/misw4204-sw-en-la-nube/global/healthChecks/hola-mundo-health-check --initial-delay=300 --no-force-update-on-repair --standby-policy-mode=manual --list-managed-instances-results=PAGELESS && gcloud beta compute instance-groups managed set-autoscaling hola-mundo-group --project=misw4204-sw-en-la-nube --region=us-central1 --mode=on --min-num-replicas=1 --max-num-replicas=5 --target-cpu-utilization=0.7 --cool-down-period=60